<Window x:Class="uygulama11.seviye3Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:uygulama11"
        mc:Ignorable="d"
        Title="seviye3Window" Height="180" Width="320"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="BtnDolar" Content= "Dolar" Grid.Row="0" Grid.Column="0" Margin="20" Click="BtnDolar_Click"/>
        <Button x:Name="BtnEuro" Content="Euro" Grid.Row="0" Grid.Column="1" Margin="20" Click="BtnEuro_Click"/>
        <Button x:Name="BtnDiger" Content="Diğer" Grid.Row="1" Grid.Column="0" Margin="20" Click="BtnDiger_Click"/>
        <Button x:Name="BtnAlakasız" Content="Alakasız" Grid.Row="1" Grid.Column="1" Margin="20" Click="BtnAlakasız_Click" />
    </Grid>
</Window>
